{
  "manifest_version": 2,

  "name": "Oxford Learner's Dictionaries Popup",
  "description": "Oxford Learner's Dictionaries Popup",
  "version": "1.0.2",
  "icons": {
      "16": "/icons/16.png",
      "48": "/icons/48.png",
      "128": "/icons/128.png"
  },

  "permissions": [
      "activeTab"
  ],
  "browser_action": {
      "default_icon": "favicon.png",
      "default_popup": "popup.html"
  }
}